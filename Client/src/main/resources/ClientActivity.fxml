<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.text.TextFlow?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.kwasheniak.ClientActivityController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <HBox fx:id="bottomHBox" layoutX="4.0" layoutY="500.0" prefHeight="100.0" prefWidth="593.0" spacing="3.0">
            <children>
                <TextFlow fx:id="textFlow" prefHeight="90.0" prefWidth="335.0" />
                <Button fx:id="buttonSend" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Send" />
                <Button fx:id="buttonAdd" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Add" />
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </HBox>
        <ScrollPane fx:id="scrollPane" layoutX="10.0" layoutY="14.0" prefHeight="482.0" prefWidth="581.0">
            <content>
                <VBox fx:id="vBox" prefHeight="200.0" prefWidth="100.0" />
            </content>
        </ScrollPane>
    </children>

</AnchorPane>
